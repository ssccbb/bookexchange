<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="theme_color">#246df5</color>
    <color name="theme_green">#03ac8e</color>
    <color name="theme_red">#ce405b</color>
    <declare-styleable name="Loading">
        <attr format="string" name="loading_text"/>
        <attr format="integer" name="loading_text_size"/>
        <attr format="color" name="loading_text_color"/>
        <attr format="color" name="loading_stroke_color"/>
        <attr format="color" name="loading_solid_color"/>
    </declare-styleable>
    <declare-styleable name="TagFlowLayout">
        <attr format="integer" name="max_select"/>
        <attr name="tag_gravity">
            <enum name="left" value="-1"/>
            <enum name="center" value="0"/>
            <enum name="right" value="1"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="VerticalRollingTextView">
        <!--两次动画之间的间隔-->
        <attr format="integer" name="animInterval"/>
        <!--展示条目的个数-->
        <attr format="integer" name="itemCount"/>
        <!--多行显示时自适应文字的最大文字大小-->
        <attr format="dimension" name="maxTextSize"/>
        <!--多行显示时自适应文字的最小文字大小-->
        <attr format="dimension" name="minTextSize"/>
    </declare-styleable>
